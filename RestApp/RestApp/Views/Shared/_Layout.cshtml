<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <!-- bootstrap -->
    <link rel="stylesheet" type="text/css" href="~/Content/bootstrap/bootstrap.min.css" />

    <!-- libraries -->
    <link rel="stylesheet" type="text/css" href="~/Content/libs/font-awesome.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/libs/nanoscroller.css" />
    <!-- global styles -->
    <link rel="stylesheet" type="text/css" href="~/Content/compiled/theme_styles.css" />
    <!-- this page specific styles -->
    <link rel="stylesheet" href="~/Content/libs/fullcalendar.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/libs/fullcalendar.print.css" type="text/css" media="print" />
    <link rel="stylesheet" href="~/Content/compiled/calendar.css" type="text/css" media="screen" />
    <link rel="stylesheet" href="~/Content/libs/morris.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/libs/daterangepicker.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/libs/jquery-jvectormap-1.2.2.css" type="text/css" />

    <!-- Favicon -->
    <link type="image/x-icon" href="favicon.png" rel="shortcut icon" />
    <!-- google font libraries -->
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,600,700,300|Titillium+Web:200,300,400' rel='stylesheet' type='text/css'>
    <!--[if lt IE 9]>
        <script src="js/html5shiv.js"></script>
        <script src="js/respond.min.js"></script>
    <![endif]-->

    <!-- Angular2 Code -->
    <base href="~/">
    
    
    
    

</head>
<body>
   
    @RenderBody()
    @RenderSection("scripts", required: false)
</body>
</html>
